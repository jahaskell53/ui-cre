# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your_supabase_publishable_key
SUPABASE_SECRET_KEY=your_supabase_secret_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# AWS S3 (Lightsail Object Storage)
ACCESS_KEY=your_aws_access_key
SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=openmidmarket

# Mapbox
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=your_mapbox_token

# Email
SMTP_PASS=
SMTP_HOST=
SMTP_PORT=
SMTP_USER=
SMTP_FROM=

# Nylas Email/Calendar Integration
NYLAS_API_KEY=your_nylas_api_key
NYLAS_CLIENT_ID=your_nylas_client_id
NYLAS_REDIRECT_URI=http://localhost:3000/api/auth/nylas/callback

# Nylas Sync Limits (optional, defaults to 600)
NYLAS_EMAIL_SYNC_LIMIT=600
NYLAS_CALENDAR_SYNC_LIMIT=600

# Google Meet Service Account (for automatic Meet link creation)
GOOGLE_SERVICE_ACCOUNT_EMAIL=meet-creator@your-project.iam.gserviceaccount.com
GOOGLE_SERVICE_ACCOUNT_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"
GOOGLE_MEET_HOST_EMAIL=meetings@yourcompany.com
